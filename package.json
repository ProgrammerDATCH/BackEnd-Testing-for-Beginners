{
  "name": "backendtesting",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha -require ts-node/register 'src/modules/**/*.spec.ts' --timeout 300000 --exit",
    "start": "nodemon src/index.ts"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "all": true,
    "include": [
      "src/**/!(*.test.*).[tj]s?(x)"
    ],
    "exclude": [
      "src/modules/**/test/*.ts"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "coverage",
    "lines": 40
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "chai": "^4.3.4",
    "chai-http": "^4.4.0",
    "express": "^4.19.2",
    "mocha": "^10.4.0",
    "nodemon": "^3.1.0",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/chai": "^4.3.16",
    "@types/chai-http": "^4.2.0",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.6"
  }
}
